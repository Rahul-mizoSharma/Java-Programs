import java.util.Scanner;
public class NumberSystem {
	
	public static void main(String [] args) {
		
		int n;
		Scanner sc =new Scanner(System.in);
        System.out.print("Enter the number : ");
        n = sc.nextInt();

        for (int i = 1; i <=n; i++) {
            if(n%i==0) {
            	System.out.println(i+" is the factor of "+n);
            }
        }
        sc.close(); 
	}
}
