import java.util.Scanner;
public class NumberSystem {
	
	public static void main(String [] args) {
		
		int n;
	
		Scanner sc =new Scanner(System.in);
        System.out.print("Enter the number : ");
        n = sc.nextInt();
        int sum=0;
        for (int i = 1; i <=n; i++) {
            if(n%i==0) {
            	sum = sum +i;
            }
        }
        System.out.println(sum); 
        sc.close();
	}
}
