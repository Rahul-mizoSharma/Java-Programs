import java.util.Scanner;

public class Swapping2numbers {
	
	public static void main(String[] args) {
		
		int a=10, b=20;
		
		System.out.println("Before swapping the values are "+a+" "+b);
		
		int temp = a;
		a = b;
		b = temp;
		
		System.out.println("After swapping the values becomes "+a+" "+b);
		}

}
